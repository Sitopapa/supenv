{
    "name": "sitopapa/supenv",
    "description": "A powerful, zero-dependency PHP Env Manager & Encrypter.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Sitopapa\\Supenv\\": "src/"
        }
    },
    "bin": [
        "bin/supenv"
    ],
    "require": {
        "php": "^8.1",
        "ext-sodium": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    },
    "autoload-dev": {
        "psr-4": {
            "Sitopapa\\Supenv\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "supenv": "supenv",
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-html coverage",
        "test:filter": "phpunit --filter"
    }
}